{
  "name": "portfolio-website",
  "version": "0.0.0",
  "engines": {
    "npm": "8.x",
    "node": "16.x"
  },
  "scripts": {
    "ng": "ng",
    "heroku-postbuild": "ng build --configuration production --output-path docs --base-href /",
    "start": "node index.js",
    "dev": "ng serve --prod",
    "dev2": "nodemon --watch src --exec npm run dev:fresh",
    "dev:fresh": "npm run build-local && npm run start",
    "watch": "npm run build-local && node index.js",
    "build": "ng build --configuration production --output-path docs --base-href /",
    "build-local": "ng build --output-path docs --base-href http://localhost:4200/",
    "build-git": "ng build --output-path docs --base-href https://musilix.github.io/Neo-Portfolio/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:netlify": "netlify dev",
    "preview:netlify": "netlify deploy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.3.0",
    "@angular/common": "^14.3.0",
    "@angular/compiler": "^14.3.0",
    "@angular/core": "^14.3.0",
    "@angular/forms": "^14.3.0",
    "@angular/platform-browser": "^14.3.0",
    "@angular/platform-browser-dynamic": "^14.3.0",
    "@angular/platform-server": "^14.3.0",
    "@angular/router": "^14.3.0",
    "bootstrap": "^5.2.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-sslify": "^1.2.0",
    "fs": "0.0.2",
    "node-fetch": "~2.6.9",
    "nodemailer": "^6.9.1",
    "path": "^0.12.7",
    "rxjs": "~6.6.7",
    "tslib": "^2.5.0",
    "typescript": "~4.8.4",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.11",
    "@angular/cli": "^14.2.11",
    "@angular/compiler-cli": "^14.3.0",
    "@angular/language-service": "^14.3.0",
    "@types/jasmine": "~3.10.7",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^17.0.45",
    "angular-cli-ghpages": "^1.0.5",
    "codelyzer": "^6.0.2",
    "dotenv": "^16.0.3",
    "netlify-cli": "^13.2.2",
    "nodemon": "^2.0.22",
    "protractor": "~7.0.0",
    "ts-node": "~10.9.1",
    "tslint": "~6.1.2"
  },
  "browserslist": [
    "defaults",
    "> .5%",
    "Firefox ESR",
    "last 2 versions",
    "not dead"
  ]
}