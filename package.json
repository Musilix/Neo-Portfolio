{
  "name": "portfolio-website",
  "version": "0.0.0",
  "engines": {
    "npm": "8.x",
    "node": "16.x"
  },
  "scripts": {
    "ng": "ng",
    "heroku-postbuild": "ng build --configuration production --output-path docs --base-href /",
    "start": "node index.js",
    "dev": "nodemon --watch src --exec npm run dev:fresh",
    "dev:fresh": "npm run build-local && npm run start",
    "watch": "npm run build-local && node index.js",
    "build": "ng build --configuration production --output-path docs --base-href /",
    "build-local": "ng build --output-path docs --base-href http://localhost:4200/",
    "build-git": "ng build --output-path docs --base-href https://musilix.github.io/Neo-Portfolio/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.3",
    "@angular/common": "~13.1.3",
    "@angular/compiler": "~13.1.3",
    "@angular/core": "~13.1.3",
    "@angular/forms": "~13.1.3",
    "@angular/platform-browser": "~13.1.3",
    "@angular/platform-browser-dynamic": "~13.1.3",
    "@angular/platform-server": "^13.1.3",
    "@angular/router": "~13.1.3",
    "bootstrap": "^5.1.3",
    "cors": "^2.8.5",
    "dotenv": "^14.2.0",
    "express": "^4.17.2",
    "express-sslify": "^1.2.0",
    "fs": "0.0.1-security",
    "node-fetch": "~2.6.1",
    "nodemailer": "^6.7.2",
    "path": "^0.12.7",
    "rxjs": "~6.5.5",
    "tslib": "^2.3.1",
    "typescript": "~4.5.5",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.10",
    "@angular/cli": "~13.1.4",
    "@angular/compiler-cli": "~13.1.3",
    "@angular/language-service": "~13.1.3",
    "@types/jasmine": "~3.10.3",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^17.0.10",
    "angular-cli-ghpages": "^1.0.0",
    "codelyzer": "^6.0.2",
    "nodemon": "^2.0.19",
    "protractor": "~7.0.0",
    "ts-node": "~10.4.0",
    "tslint": "~6.1.2",
    "typescript": "~4.5.5"
  },
  "browserslist": [
    "defaults",
    "> .5%",
    "Firefox ESR",
    "last 2 versions",
    "not dead"
  ]
}
